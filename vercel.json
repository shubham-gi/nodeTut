{
    "version": 2,
    "builds": [
     {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
     },
     {
        "src": "build/**",
        "use": "@vercel/static",
        "config": { "includeFiles": ["dist/**"] }
       }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "index.js"
     }
    ]
   }