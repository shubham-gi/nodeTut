// {
//   "version": 2,
//   "rewrites": [{ "source": "/(.*)", "destination": "/index.js" }]
// }
{
    "version": 2,
    "builds": [
     {
      "src": "app.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
     }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "app.js"
     }
    ]
   }